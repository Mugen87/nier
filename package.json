{
  "name": "nier",
  "version": "1.0.0",
  "description": "Hacking game from Nier: Automata",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c \"config/rollup/config.js\"",
    "build-prod": "rollup -c \"config/rollup/config.js\" && google-closure-compiler build/bundle.js > build/bundle.min.js",
    "dev": "concurrently \"rollup -c \"config/rollup/config.js\" -w\" \"serve\"",
    "lint": "eslint src --ext .js",
    "start": "npm run dev"
  },
  "eslintConfig": {
    "extends": "mdcs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mugen87/nier.git"
  },
  "author": "Mugen87",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mugen87/nier/issues"
  },
  "homepage": "https://github.com/Mugen87/nier#readme",
  "devDependencies": {
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-mdcs": "^4.2.3",
    "google-closure-compiler": "^20190929.0.0",
    "rollup": "^2.3.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "serve": "^11.3.0"
  },
  "dependencies": {
    "three": "0.110.0",
    "yuka": "0.3.0"
  }
}
